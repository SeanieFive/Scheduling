<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="370.0" prefWidth="454.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="scheduling.AddAddressController">
   <children>
      <VBox layoutX="12.0" layoutY="4.0" spacing="20.0">
         <children>
            <Label layoutX="10.0" layoutY="10.0" text="Add Address">
               <font>
                  <Font size="24.0" />
               </font>
            </Label>
            <Separator prefWidth="200.0" />
            <GridPane hgap="20.0" layoutX="294.0" layoutY="81.0" prefHeight="190.0" prefWidth="419.0" vgap="10.0">
               <children>
                  <TextField fx:id="address1Field" promptText="Address Line 1" GridPane.columnIndex="1" />
                  <TextField fx:id="address2Field" promptText="Address Line 2" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                  <ComboBox fx:id="cityComboBox" prefWidth="150.0" GridPane.columnIndex="1" GridPane.rowIndex="2" />
                  <TextField fx:id="postalCodeField" promptText="Postal Code" GridPane.columnIndex="1" GridPane.rowIndex="3" />
                  <TextField fx:id="phoneNumberField" promptText="Phone Number" GridPane.columnIndex="1" GridPane.rowIndex="4" />
                  <Label text="Address Line 1:" />
                  <Label text="Address Line 2:" GridPane.rowIndex="1" />
                  <Label text="City:" GridPane.rowIndex="2" />
                  <Label text="Postal Code:" GridPane.rowIndex="3" />
                  <Label text="Phone Number:" GridPane.rowIndex="4" />
               </children>
               <columnConstraints>
                  <ColumnConstraints halignment="RIGHT" hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                  <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
               </columnConstraints>
               <rowConstraints>
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
               </rowConstraints>
            </GridPane>
            <HBox alignment="TOP_RIGHT" layoutX="10.0" layoutY="10.0" spacing="10.0">
               <children>
                  <Button fx:id="addButton" mnemonicParsing="false" onAction="#addSelected" text="Add" />
                  <Button fx:id="cancelButton" mnemonicParsing="false" onAction="#cancelSelected" text="Cancel" />
               </children>
               <padding>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </padding>
            </HBox>
         </children>
         <padding>
            <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
         </padding>
      </VBox>
   </children>
</AnchorPane>
